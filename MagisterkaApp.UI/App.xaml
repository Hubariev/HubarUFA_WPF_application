<Application x:Class="MagisterkaApp.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MagisterkaApp.UI"
             xmlns:converters="clr-namespace:MagisterkaApp.UI.Miscellaneous">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Others.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <converters:StringToSolidColorBrushConverter x:Key="StringToSolidColorBrushConverter" />

        </ResourceDictionary>
    </Application.Resources>
</Application>
